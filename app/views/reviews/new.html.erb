<h1>Reviews#new</h1>
<%= form_for @review do |f|%>
<%=f.text_field :title, placeholder: 'Topic'%><br />
<%=f.text_area :body, placeholder: 'Comment'%><br />
<%=f.number_field :rating, placeholder: 'Rating'%><br />
<%=f.text_field :bar_id, placeholder: 'Bar Name'%><br />

<%= f.submit %><br />
<% end %>


